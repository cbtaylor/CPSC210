package ca.ubc.cs.cpsc210.meetup.exceptions;

// Represent a problem with student information

public class IllegalStudentException extends RuntimeException {
	
	/**
	 * Constructor
	 */
	public IllegalStudentException() {
		super();
	}
	
	/**
	 * Constructor
	 * @param msg The message to put in the exception object
	 */
	public IllegalStudentException(String msg) {
		super(msg);
	}

}
