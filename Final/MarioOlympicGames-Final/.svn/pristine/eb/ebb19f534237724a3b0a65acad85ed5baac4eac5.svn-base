package characters;

import sports.PoolSport;

public class Luigi extends Mario implements PoolSport {

	/**
	 * Swim!
	 * EFFECTS: Writes information to the console
	 */
	public void swim() {
		System.out.println("Luigi takes three strokes!");
	}

	/**
	 * Return the speed at which the running occurs for this character
	 * EFFECTS: Returns a speed that is >= 5
	 */
	public int getSpeed() {
		return 5;
	}
}
